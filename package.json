    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "typewriter-editor": "https://github.com/gllms/typewriter/tarball/gllms"
  }
